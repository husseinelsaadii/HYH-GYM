r class=\"app-footer\">\r\n    <div class=\"footer-content\">\r\n        <div class=\"footer-logo\">\r\n            <img src=\"images/GYM%20Logo.png\" alt=\"HYH Logo\">\r\n        </div>\r\n        <div class=\"footer-links\">\r\n            <div class=\"footer-column\">\r\n                <h3>NAVIGATE</h3>\r\n                <ul>\r\n                    <li><a href=\"about-us.html\">What is HYH?</a></li>\r\n                    <li><a href=\"cardio.html\">Cardio</a></li>\r\n                    <li><a href=\"resistance.html\">Resistance</a></li>\r\n<!--                    <li><a href=\"schedule.html\">Schedule</a></li>-->\r\n                </ul>\r\n            </div>\r\n            <div class=\"footer-column\">\r\n                <h3>JOIN US</h3>\r\n                <ul>\r\n                    <li><a href=\"membership.html\">Membership</a></li>\r\n                    <li><a href=\"join.html\">Free Trial</a></li>\r\n                    <li><a href=\"location.html\">Locations</a></li>\r\n                    <li><a href=\"contact-us.html\">Contact</a></li>\r\n                </ul>\r\n            </div>\r\n            <div class=\"footer-column\">\r\n                <h3>FOLLOW US</h3>\r\n                <div class=\"social-icons\">\r\n                    <a href=\"#\" class=\"social-icon\"><i class=\"fab fa-instagram\"></i></a>\r\n                    <a href=\"#\" class=\"social-icon\"><i class=\"fab fa-facebook\"></i></a>\r\n                    <a href=\"#\" class=\"social-icon\"><i class=\"fab fa-twitter\"></i></a>\r\n                    <a href=\"#\" class=\"social-icon\"><i class=\"fab fa-youtube\"></i></a>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"footer-bottom\">\r\n        <p>&copy; 2025 HYH TRAINING. ALL RIGHTS RESERVED.</p>\r\n    </div>\r\n</footer>\r\n\r\n<!-- AOS Animation Script -->\r\n<script src=\"https://unpkg.com/aos@2.3.1/dist/aos.js\"></script>\r\n<script src=\"js/cardio.js\"></script>\r\n</body>